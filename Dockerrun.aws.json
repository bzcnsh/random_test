{
  "AWSEBDockerrunVersion": 2,
  "volumes": [
    {
      "name": "tmp",
      "host": {
        "sourcePath": "/tmp"
      }
    }
  ],
  "containerDefinitions": [
    {
      "name": "dummy-nginx",
      "image": "347967168134.dkr.ecr.us-east-1.amazonaws.com/codebuild01:latest",
      "environment": [
        {
          "name": "Container",
          "value": "PHP"
        }
      ],
      "essential": true,
      "memory": 128,
      "mountPoints": [
        {
          "sourceVolume": "tmp",
          "containerPath": "/tmp/12",
          "readOnly": true
        }
      ],
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ]
    },
    {
      "name": "redis",
      "image": "redis:4.0.9",
      "portMappings": [
        {
          "hostPort": 6379,
          "containerPort": 6379
        }
      ]
    }
  ]
}

